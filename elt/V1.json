    {
      "parameters": {
        "operation": "executeQuery",
        "query": "-- Checkpoint #4: Orphan Check\nSELECT \n    (SELECT COUNT(*) FROM staging.clean_sales s \n     WHERE NOT EXISTS (SELECT 1 FROM dwh.dim_product p WHERE p.stock_code = s.stockcode)) as orphan_products,\n    (SELECT COUNT(*) FROM staging.clean_sales s \n     WHERE NOT EXISTS (SELECT 1 FROM dwh.dim_date d WHERE d.full_date = s.invoice_date)) as orphan_dates;",
        "options": {}
      },
