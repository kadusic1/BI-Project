        }
      }
    },
    {
      "parameters": {
        "operation": "executeQuery",
        "query": "-- Load Product Dimension\nINSERT INTO dwh.dim_product (stock_code, description, category)\nSELECT DISTINCT\n    stockcode,\n    description,\n    COALESCE(\n        (SELECT category FROM staging.product_category_lookup \n         WHERE description ILIKE '%' || keyword || '%' \n         ORDER BY priority ASC LIMIT 1),\n        'Uncategorized'\n    ) as category\nFROM staging.clean_sales\nON CONFLICT (stock_code) DO NOTHING;",
        "options": {}
      },
      "id": "8aae42ba-58d3-4149-b023-3030bd6c59e5",
      "name": "Load dim_product",
      "type": "n8n-nodes-base.postgres",
